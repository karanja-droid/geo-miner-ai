{
  "name": "geominer-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "supabase:local": "./scripts/supabase-setup.sh local",
    "supabase:setup": "./scripts/supabase-setup.sh",
    "supabase:test": "./scripts/supabase-setup.sh test",
    "test": "jest",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:mutation": "stryker run",
    "test:mutation:components": "stryker run --configFile stryker.components.conf.json",
    "test:mutation:lib": "stryker run --configFile stryker.lib.conf.json",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@axe-core/playwright": "latest",
